{
	"name": "@snowcrystals/iglo",
	"version": "1.2.0",
	"description": "A template used for every single repository in this organization 📏",
	"author": "ijsKoud <github@ijskoud.dev>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs",
		"types": "./dist/index.d.ts"
	},
	"files": [
		"dist/"
	],
	"homepage": "https://github.com/snowcrystals/template",
	"scripts": {
		"build": "tsup src",
		"build:watch": "tsc --watch",
		"start:dev": "nodemon ./dist",
		"start": "node ./dist",
		"lint": "eslint src",
		"postinstall": "is-ci || husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"bump": "cliff-jumper"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-angular": "^17.4.4",
		"@favware/cliff-jumper": "^1.10.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/lodash": "^4.14.191",
		"@types/node": "^18.14.1",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"eslint": "^8.35.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.1.2",
		"nodemon": "^2.0.20",
		"pinst": "^3.0.0",
		"prettier": "^2.8.4",
		"tsup": "^6.6.3",
		"typescript": "4.9.5"
	},
	"dependencies": {
		"@snowcrystals/icicle": "^1.0.2",
		"discord.js": "^14.7.1",
		"lodash": "^4.17.21"
	},
	"engines": {
		"node": ">= v18.14.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.4.1"
}
