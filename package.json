{
	"name": "@snowcrystals/iglo",
	"version": "1.2.8",
	"description": "A template used for every single repository in this organization 📏",
	"author": "ijsKoud <github@ijskoud.dev>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs",
		"types": "./dist/index.d.ts"
	},
	"files": [
		"dist/"
	],
	"homepage": "https://github.com/snowcrystals/iglo",
	"scripts": {
		"build": "tsup src",
		"build:watch": "tsc --watch",
		"start:dev": "nodemon ./dist",
		"start": "node ./dist",
		"lint": "eslint src",
		"docs": "typedoc-json-parser",
		"postinstall": "is-ci || husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"bump": "cliff-jumper"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-angular": "^17.7.0",
		"@favware/cliff-jumper": "^2.1.1",
		"@sapphire/eslint-config": "^5.0.1",
		"@sapphire/ts-config": "^4.0.1",
		"@snowcrystals/prettier-config": "^1.0.0",
		"@types/lodash": "^4.14.197",
		"@types/node": "^18.17.6",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"@typescript-eslint/parser": "^6.4.0",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^14.0.0",
		"nodemon": "^3.0.1",
		"pinst": "^3.0.0",
		"prettier": "^3.0.2",
		"tsup": "^7.2.0",
		"typedoc": "^0.24.8",
		"typedoc-json-parser": "^8.2.0",
		"typescript": "5.1.6"
	},
	"dependencies": {
		"@snowcrystals/icicle": "^2.0.4",
		"discord.js": "^14.13.0",
		"lodash": "^4.17.21"
	},
	"engines": {
		"node": ">= v18.16.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@3.6.3"
}
