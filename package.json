{
	"name": "@snowcrystals/iglo",
	"version": "1.2.8",
	"description": "A template used for every single repository in this organization 📏",
	"author": "ijsKoud <github@ijskoud.dev>",
	"license": "MIT",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.cjs",
		"types": "./dist/index.d.ts"
	},
	"files": [
		"dist/"
	],
	"homepage": "https://github.com/snowcrystals/iglo",
	"scripts": {
		"build": "tsup src",
		"build:watch": "tsc --watch",
		"start:dev": "nodemon ./dist",
		"start": "node ./dist",
		"lint": "eslint src",
		"docs": "typedoc-json-parser",
		"postinstall": "is-ci || husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"bump": "cliff-jumper"
	},
	"resolutions": {
		"load-tsconfig": "^0.2.5"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^3.0.3",
		"@sapphire/eslint-config": "^5.0.5",
		"@sapphire/ts-config": "^5.0.1",
		"@snowcrystals/prettier-config": "^1.0.0",
		"@types/lodash": "^4.17.7",
		"@types/node": "^20.16.5",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^15.2.10",
		"nodemon": "^3.1.4",
		"pinst": "^3.0.0",
		"prettier": "^3.3.3",
		"tsup": "^8.2.4",
		"typedoc": "^0.26.6",
		"typedoc-json-parser": "^10.1.5",
		"typescript": "5.5.4"
	},
	"dependencies": {
		"@snowcrystals/icicle": "^2.0.4",
		"discord.js": "^14.16.2",
		"lodash": "^4.17.21"
	},
	"engines": {
		"node": ">= v18.16.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@4.4.1"
}
